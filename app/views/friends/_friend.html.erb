<div id="<%= dom_id friend %>">

  <p>
    <strong>Name:</strong>
    <%= link_to friend.first_name + " " + friend.last_name , friend %>
    <%#= friend.first_name %>
    <%#= friend.last_name %>
  </p>


  <p>
    <strong>E mail:</strong>
    <%= friend.e_mail %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= friend.twitter %>
  </p>

   <p>
    <strong>User Id:</strong>
    <%= friend.user_id %>
  </p>

</div>
